<Pane id="CAsel" minWidth="-Infinity" stylesheets="@../resources/style.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="CAutomatasWindow.CAutomatasController">
